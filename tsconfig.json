{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./compiled_ts/app",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "noUnusedLocals": true,
    "noImplicitAny": true
  }
}
