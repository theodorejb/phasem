{
    "name": "theodorejb/phasem",
    "description": "Quickly get started with a PHP/Angular/Slim/Nginx/MySQL web app",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "defuse/php-encryption": "^v2.3.1",
        "shrikeh/teapot": "^2.3.1",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "theodorejb/multi-factor": "^1.0.1",
        "theodorejb/peachy-sql": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "psalm/plugin-phpunit": "^0.16.1",
        "vimeo/psalm": "^4.20"
    },
    "autoload": {
        "classmap": ["AppConfig.php"],
        "psr-4": {"Phasem\\": "php/"}
    }
}
